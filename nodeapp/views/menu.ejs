<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    
    <table>
      <tr>
        <th>Name</th>
        <th>Image</th>
        <th>Price</th>
      </tr>
      <% if(data.length!=0){ data.forEach(function(data){ %> 
        <tr>
          <td><%=data.name %> </td>
          <td><img src="data:image/jpeg;base64,<%- data.img %>" /> </td>
          <td><%=data.price %></td>
        </tr>
      <% })} else{ %>
      <h2>Es Konnten Keinde Daten Ausgelesen werden</h2>
      <% } %>
    </table>

  </body>
</html>